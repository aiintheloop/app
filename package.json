{
  "name": "nextjs-subscription-payments",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npx concurrently \"next dev\" \"quirrel\"",
    "build": "next build",
    "start": "npx concurrently \"next start\" \"quirrel\"",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name=saas-starter",
    "merge-files": "ts-node --esm merge-files.mts",
    "openapi:yaml": "typeconv -v -f ts -t oapi --oapi-format yaml --output-extension swagger.yaml --output-directory public/schemas/openapi .build/models/*.ts",
    "build-models": "yarn run merge-files && yarn run openapi:yaml",
    "generate-swagger": "yarn run build-models && yarn next-swagger-doc-cli next-swagger-doc.json"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@mui/x-data-grid": "^5.17.20",
    "@novu/node": "^0.11.0",
    "@novu/notification-center": "^0.11.0",
    "@slack/oauth": "^2.6.0",
    "@stripe/stripe-js": "1.22.0",
    "@supabase/auth-helpers-nextjs": "^0.4.1",
    "@supabase/auth-helpers-react": "^0.3.0",
    "@supabase/auth-ui-react": "^0.2.6",
    "@supabase/supabase-js": "^2.0.0",
    "@types/jest": "^29.5.2",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.3.3",
    "classnames": "2.3.1",
    "concurrently": "^7.6.0",
    "core-js": "^3.27.2",
    "daisyui": "^2.51.6",
    "jest": "^29.5.0",
    "mobx": "^6.7.0",
    "moment": "^2.29.4",
    "next": "^12.2.5",
    "next-swagger-doc": "^0.3.6",
    "nextjs-progressbar": "^0.0.16",
    "quirrel": "^1.13.1",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "17.0.2",
    "react-merge-refs": "1.1.0",
    "react-toastify": "^9.1.1",
    "redoc": "^2.0.0",
    "stripe": "8.201.0",
    "styled-components": "^5.3.6",
    "swr": "1.2.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "3.0.18",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "uuid4": "^2.0.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/node": "^17.0.13",
    "@types/react": "17.0.2",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "supabase": "^1.33.0",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
